<h1>Sign Up</h1>

<%= form_for :user do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :first_name %>
  <%= f.text_field :firstname %>

  <%= f.label :last_name %>
  <%= f.text_field :lastname %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit :submit %>
<% end %>
